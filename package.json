{
  "name": "setup-butler",
  "version": "0.0.0",
  "private": true,
  "description": "A GitHub Action that sets up itch.io's butler tool",
  "main": "build/main.js",
  "scripts": {
    "package": "tsc && ncc build --minify",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.md": "markdownlint **/*.md --ignore node_modules",
    "**/*.ts": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdno/setup-butler.git"
  },
  "keywords": [
    "github",
    "action",
    "itch.io",
    "butler"
  ],
  "author": "Jan David Nose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdno/setup-butler/issues"
  },
  "homepage": "https://github.com/jdno/setup-butler#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "markdownlint-cli": "^0.31.1",
    "prettier": "2.6.2",
    "typescript": "^4.6.3"
  }
}
