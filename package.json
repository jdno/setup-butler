{
  "name": "setup-butler",
  "version": "1.0.0",
  "private": true,
  "description": "Set up itch.io's butler tool in your GitHub Actions workflow",
  "main": "build/main.js",
  "scripts": {
    "package": "tsc && ncc build --minify",
    "prepare": "husky install",
    "test": "jest"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.md": "markdownlint **/*.md --ignore node_modules",
    "**/*.ts": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdno/setup-butler.git"
  },
  "keywords": [
    "github",
    "action",
    "itch.io",
    "butler"
  ],
  "author": "Jan David Nose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdno/setup-butler/issues"
  },
  "homepage": "https://github.com/jdno/setup-butler#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.8",
    "markdownlint-cli": "^0.31.1",
    "prettier": "2.6.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^1.7.2"
  }
}
